<h1>Listing section_yields</h1>

<table>
  <thead>
    <tr>
      <th>Budget</th>
      <th>Actual</th>
      <th>Year</th>
      <th>Section</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @section_yields.each do |section_yield| %>
      <tr>
        <td><%= section_yield.budget %></td>
        <td><%= section_yield.actual %></td>
        <td><%= section_yield.year %></td>
        <td><%= section_yield.section %></td>
        <td><%= link_to 'Show', section_yield %></td>
        <td><%= link_to 'Edit', edit_section_yield_path(section_yield) %></td>
        <td><%= link_to 'Destroy', section_yield, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Section yield', new_section_yield_path %>
