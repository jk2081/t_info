<h1>Listing yield_categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @yield_categories.each do |yield_category| %>
      <tr>
        <td><%= yield_category.name %></td>
        <td><%= link_to 'Show', yield_category %></td>
        <td><%= link_to 'Edit', edit_yield_category_path(yield_category) %></td>
        <td><%= link_to 'Destroy', yield_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Yield category', new_yield_category_path %>
