<h1>Listing section_yield_categories</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Yield category</th>
      <th>Section</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @section_yield_categories.each do |section_yield_category| %>
      <tr>
        <td><%= section_yield_category.year %></td>
        <td><%= section_yield_category.yield_category %></td>
        <td><%= section_yield_category.section %></td>
        <td><%= link_to 'Show', section_yield_category %></td>
        <td><%= link_to 'Edit', edit_section_yield_category_path(section_yield_category) %></td>
        <td><%= link_to 'Destroy', section_yield_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Section yield category', new_section_yield_category_path %>
