<h1>Listing section_divisions</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Section</th>
      <th>Division</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @section_divisions.each do |section_division| %>
      <tr>
        <td><%= section_division.year %></td>
        <td><%= section_division.section %></td>
        <td><%= section_division.division %></td>
        <td><%= link_to 'Show', section_division %></td>
        <td><%= link_to 'Edit', edit_section_division_path(section_division) %></td>
        <td><%= link_to 'Destroy', section_division, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Section division', new_section_division_path %>
