<h1>Listing section_prune_types</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Section</th>
      <th>Prune type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @section_prune_types.each do |section_prune_type| %>
      <tr>
        <td><%= section_prune_type.year %></td>
        <td><%= section_prune_type.section %></td>
        <td><%= section_prune_type.prune_type %></td>
        <td><%= link_to 'Show', section_prune_type %></td>
        <td><%= link_to 'Edit', edit_section_prune_type_path(section_prune_type) %></td>
        <td><%= link_to 'Destroy', section_prune_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Section prune type', new_section_prune_type_path %>
